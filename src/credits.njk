---
bandName: Credits
slug: credits
elements: ['rectangle-gradient', 'triangle-rectangle']
---

{% extends "layouts/base.njk" %}

{% block content %}
  <div class="stage">
    <main class="band {{ slug }}">
      <div>
        <div class="band__art">
          <artistic-triangle-rectangle
          width="71.6"
          height="71.6"
          topColor="#000211"
          rightColor="#dfe2da"
          bottomColor="#dfe2da"
          leftColor="#000211"
          borderStyle="double"
        >
            <artistic-triangle-rectangle
            width="35.8"
            height="35.8"
            topColor="transparent"
            rightColor="#000211"
            bottomColor="#000211"
            leftColor="transparent"
            borderStyle="solid"
          ></artistic-triangle-rectangle>
          </artistic-triangle-rectangle>
        </div>
        <div class="band__information">
          <h1>Mike Joyce</h1>
          <div class="details">
            <div class="venue">
              <a href="https://www.stereotypenyc.com/">Stereotype Design</a>
              <div>39 Jane Street, NYC</div>
            </div>
            <div class="links">
              <div>
                <a href="https://twitter.com/mikejoycenyc">twitter</a>
              </div>
              <div>
                <a href="https://www.swissted.com/">swissted.com</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>
  </div>
{% endblock %}
